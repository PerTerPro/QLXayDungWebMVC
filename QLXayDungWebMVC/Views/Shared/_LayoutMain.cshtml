    QLXayDungWebMVC.Models.QLCTXayDungContext db = new QLXayDungWebMVC.Models.QLCTXayDungContext();
    QLXayDungWebMVC.Models.User usr = new QLXayDungWebMVC.Models.User();
    QLXayDungWebMVC.Dapper.UserRepo usrRP = new QLXayDungWebMVC.Dapper.UserRepo();
